(library
 ((name        snark_keys)
  (public_name snark_keys)
  (flags (:standard -short-paths))
  (libraries
    ( async
      transaction_snark
      dummy_values
      blockchain_snark
      core
    ))
  (preprocess (pps (ppx_jane ppxlib.runner)))))

(rule
 ((targets (snark_keys.ml))
  (deps    (gen_keys/gen_keys.exe))
  (action  (run ${<} ${@}))))

(jbuild_version 1)
