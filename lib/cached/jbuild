(library
 ((name cached)
  (public_name cached)
  (flags (:standard -short-paths -warn-error -58))
  (library_flags (-linkall))
  (inline_tests)
  (libraries
    ( core
      storage
      async ))
  (preprocess (pps (ppx_jane)))
  (synopsis "Cache computation results on disk")))

