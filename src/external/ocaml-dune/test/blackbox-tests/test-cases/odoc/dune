(library
 (name foo)
 (modules foo foo2)
 (wrapped false)
 (public_name foo))

(library
 (name bar)
 (public_name bar)
 (modules bar))

(library
 (name foo_byte)
 (modules foo_byte)
 (modes byte)
 (public_name foo.byte))

(alias
 (name runtest)
 (deps _doc/_html/index.html)
 (action (echo "%{read:_doc/_html/index.html}")))

