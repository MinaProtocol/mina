(library
 (name dyn_array)
 (public_name dyn_array)
 (library_flags -linkall)
 (inline_tests)
 (libraries core extlib)
 (flags -warn-error -22)
 (preprocess
  (pps ppx_version ppx_jane bisect_ppx -- -conditional))
 (synopsis
   "dyn_array has the same capabilities as Core's DynArray and it is also serializable"))
