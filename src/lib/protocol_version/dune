(library
 (name protocol_version)
 (public_name protocol_version)
 (library_flags -linkall)
 (libraries
   ;; opam libraries
   core_kernel
   sexplib0
   bin_prot.shape
   base.caml
   base
   ppx_version.runtime
   ;; local libraries
   mina_wire_types
 )
 (preprocess
  (pps ppx_version ppx_bin_prot ppx_fields_conv ppx_sexp_conv ppx_compare ppx_deriving_yojson))
 (instrumentation (backend bisect_ppx))
 (flags -w -22) ;; to allow ppx_version to be used in a functor body.
 (synopsis "Protocol version representation"))
