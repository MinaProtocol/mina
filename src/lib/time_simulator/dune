(library
 (name time_simulator)
 (public_name time_simulator)
 (library_flags -linkall)
 (inline_tests)
 (libraries mina_intf)
 (preprocess
  (pps ppx_version ppx_jane ppx_deriving.eq))
 (instrumentation (backend bisect_ppx))
 (synopsis "Time simulator. Time moves faster if nothing is happening."))
