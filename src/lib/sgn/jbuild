(jbuild_version 1)

(library
 ((name sgn)
  (public_name sgn)
  (flags (:standard -short-paths -warn-error -58))
  (library_flags (-linkall))
  (inline_tests)
  (libraries
    ( sgn_type snark_params core_kernel ))
  (preprocess (pps (ppx_jane ppx_deriving.eq bisect_ppx -conditional)))
  (synopsis "sgn library")))

