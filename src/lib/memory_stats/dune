(library
 (name memory_stats)
 (public_name memory_stats)
 (library_flags -linkall)
 (libraries
   async_kernel
   core
   jemalloc
   logger
   o1trace
 )
 (preprocess
  (pps ppx_snarky ppx_coda ppx_version ppx_let))
 (instrumentation (backend bisect_ppx))
 (synopsis "Memory statistics for OCaml and jemalloc"))
