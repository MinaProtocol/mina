(library
 (name merkle_mask)
 (public_name merkle_mask)
 (library_flags -linkall)
 (libraries core debug_assert bitstring integers immutable_array
   merkle_ledger merkle_address yojson visualization)
 (preprocess
  (pps
   ppx_deriving.eq
   ppx_deriving.show
   ppx_deriving_yojson
   ppx_jane
   ppx_version
   bisect_ppx -- --conditional))
 (synopsis "Implementation of Merkle tree masks"))
