(library
 (name work_partitioner)
 (public_name work_partitioner)
 (library_flags -linkall)
 (libraries 
   ;; OPAM Libraries
   async 
   core
   ;; Local Libraries
   mina_base
   snark_work_lib
   transaction_snark
   work_selector)
   
 (preprocess
  (pps
   ppx_custom_printf
   ppx_deriving_yojson
   ppx_let
   ppx_sexp_conv
   ppx_version))
   
 (instrumentation
  (backend bisect_ppx))
 (synopsis "Partition work returned by Work_selector and issue them to real Snark Worker"))
