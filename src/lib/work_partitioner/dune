(library
 (name work_partitioner)
 (public_name work_partitioner)
 (library_flags -linkall)
 (libraries 
   ; OPAM libraries
   core_kernel 
   ; local libraries
   snark_work_lib
   ledger_proof
   signature_lib
   currency)
 (preprocess
  (pps
   ppx_let
   ppx_custom_printf))
   
 (instrumentation
  (backend bisect_ppx))
 (synopsis "Partition work returned by Work_selector and issue them to real Snark Worker"))
