(library
 (name mina_stdlib)
 (public_name mina_stdlib)
 (inline_tests
  (flags -verbose -show-counts))
 (modules_without_implementation generic_set sigs)
 (flags
  (:standard -w a -warn-error +a)
  -open
  Core_kernel)
 (libraries
  ; opam libraries
  base.caml
  core_kernel
  async_kernel
  async
  async_unix
  core
  sexplib0
  bin_prot.shape
  ; local libraries
  ppx_version.runtime)
 (instrumentation
  (backend bisect_ppx))
 (preprocess
  (pps ppx_jane ppx_version ppx_deriving.std ppx_mina ppx_compare))
 (synopsis "Mina standard library"))
