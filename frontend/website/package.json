{
  "name": "with-reasonml",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently 'yarn dev:reason' 'yarn dev:next'",
    "dev:reason": "bsb -clean-world -make-world -w",
    "dev:next": "next dev -p 8000",
    "clean": "bsb -clean-world",
    "build": "graphdoc -s ../../graphql_schema.json -o ./public/graphql-docs --force && bsb -make-world && next build",
    "export": "yarn build && next export",
    "start": "next start -p 8000",
    "reformat": "bsrefmt --in-place src/**/*.re"
  },
  "license": "ISC",
  "dependencies": {
    "@mdx-js/loader": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@next/mdx": "^9.1.5",
    "bs-css": "^9.0.1",
    "bs-fetch": "^0.5.0",
    "bsc-stdlib-polyfill": "Schmavery/bsc-stdlib-polyfill",
    "contentful": "^7.10.0",
    "emotion-server": "^10.0.17",
    "isomorphic-unfetch": "^3.0.0",
    "moment": "^2.26.0",
    "next": "latest",
    "next-transpile-modules": "2.3.1",
    "nextjs-morph-page": "^2.0.3",
    "react": "^16.8.6",
    "react-cookie-consent": "^3.0.0",
    "react-dom": "^16.8.6",
    "react-gtm-module": "^2.0.8",
    "reason-react": "^0.7.0",
    "rehype-katex": "^2.0.0",
    "rehype-raw": "^4.0.1",
    "rehype-react": "^4.0.1",
    "rehype-sanitize": "^3.0.0",
    "remark-math": "^2.0.0",
    "remark-parse": "^7.0.2",
    "remark-rehype": "^5.0.0",
    "remark-slug": "^5.1.2",
    "unified": "^8.4.2"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@babel/core": "^7.4.3",
    "@next/bundle-analyzer": "^9.1.4",
    "bs-platform": "7.0.0-dev.1",
    "concurrently": "^4.1.0",
    "rehype-highlight": "^3.1.0"
  },
  "resolutions": {
    "serialize-javascript": "^2.1.1"
  }
}
